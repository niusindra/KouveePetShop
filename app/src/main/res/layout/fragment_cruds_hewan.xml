<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/green_kv"
    tools:context=".View.CustomerService.ui.CrudsHewanFragment">
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/CS_fab_hewan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:src="@drawable/ic_add_black_24dp"
        android:backgroundTint="@color/green_kv"
        app:borderWidth="0dp"
        app:elevation="6dp"
        app:fabSize="normal" />

    <androidx.cardview.widget.CardView
        android:id="@+id/CS_RC_Placeholder"
        android:layout_width="match_parent"
        android:layout_height="699dp"
        android:layout_marginBottom="0dp"
        android:layout_below="@+id/layoutLog"
        android:backgroundTint="@color/real_green">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/CS_RC_Hewan"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="0dp">

        </androidx.recyclerview.widget.RecyclerView>
    </androidx.cardview.widget.CardView>
</RelativeLayout>